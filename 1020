using System; 

class URI {

    static void Main(string[] args) { 
         int dias, meses, anos;
            dias = int.Parse(Console.ReadLine());
            anos = dias / 365;
            meses = (dias - (anos * 365)) / 30;
dias = (dias - (anos * 365) - (meses * 30));
            Console.WriteLine("{0} ano(s)\n{1} mes(es)\n{2} dia(s)", anos, meses, dias);

    }

}
